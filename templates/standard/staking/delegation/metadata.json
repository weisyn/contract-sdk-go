{
  "id": "delegation",
  "name": "Delegation",
  "category": "Staking",
  "description": "委托质押合约，实现质押权的委托和取消委托，适用于委托质押、收益分配等场景",
  "tags": [
    "standard",
    "staking",
    "delegation",
    "delegate"
  ],
  "language": "go",
  "level": "standard",
  "entryFile": "main.go",
  "helpers": [
    "staking"
  ],
  "parameters": [
    {
      "name": "validator",
      "type": "address",
      "required": true,
      "description": "验证者地址"
    },
    {
      "name": "delegatee",
      "type": "address",
      "required": true,
      "description": "被委托者地址（Delegate）"
    },
    {
      "name": "amount",
      "type": "number",
      "required": true,
      "description": "委托/取消委托数量"
    }
  ],
  "risks": [
    "需要实现委托关系管理（使用状态输出）",
    "需要实现委托收益计算",
    "需要实现锁定期管理"
  ],
  "prerequisites": [
    "完成 Basic Staking 模板",
    "了解委托质押机制"
  ],
  "examples": [
    "wes contract call <contract_address> --function Delegate --params '{\"validator\":\"<address>\",\"delegatee\":\"<address>\",\"amount\":10000}'",
    "wes contract call <contract_address> --function Undelegate --params '{\"validator\":\"<address>\",\"amount\":5000}'",
    "wes contract call <contract_address> --function QueryDelegation --params '{\"validator\":\"<address>\"}'"
  ],
  "version": "1.0.0",
  "author": "WES Contract SDK Team",
  "license": "Apache-2.0",
  "sdkCompatibility": {
    "go": ">=0.1.0-alpha <0.2.0"
  },
  "sinceSdk": {
    "go": "0.1.0-alpha"
  }
}
