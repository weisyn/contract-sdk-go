{
  "id": "commodity",
  "name": "Commodity",
  "category": "RWA",
  "description": "商品代币化合约，使用 ISPC 受控机制验证和代币化商品，实现黄金、石油、农产品等商品的链上管理和交易",
  "tags": [
    "standard",
    "rwa",
    "commodity",
    "gold",
    "oil",
    "agriculture"
  ],
  "language": "go",
  "level": "standard",
  "entryFile": "main.go",
  "helpers": [
    "rwa",
    "token",
    "market"
  ],
  "parameters": [
    {
      "name": "asset_id",
      "type": "string",
      "required": true,
      "description": "资产ID（TokenizeCommodity）"
    },
    {
      "name": "total_supply",
      "type": "number",
      "required": true,
      "description": "总供应量（TokenizeCommodity）"
    },
    {
      "name": "token_id",
      "type": "string",
      "required": true,
      "description": "代币ID"
    },
    {
      "name": "to",
      "type": "address",
      "required": true,
      "description": "接收者地址（TransferCommodity）"
    },
    {
      "name": "amount",
      "type": "number",
      "required": true,
      "description": "转移数量（TransferCommodity）"
    }
  ],
  "risks": [
    "需要实现商品验证逻辑（验证服务）",
    "需要实现商品估值逻辑（估值服务）",
    "需要实现存储管理（存储位置、保管证明）"
  ],
  "prerequisites": [
    "了解 RWA 代币化概念",
    "了解 ISPC 受控外部交互机制",
    "了解商品验证和估值流程"
  ],
  "examples": [
    "wes contract call <contract_address> --function TokenizeCommodity --params '{\"asset_id\":\"commodity_001\",\"total_supply\":1000000,\"token_id\":\"COMMODITY_001\"}'",
    "wes contract call <contract_address> --function TransferCommodity --params '{\"to\":\"<address>\",\"token_id\":\"COMMODITY_001\",\"amount\":1000}'"
  ],
  "version": "1.0.0",
  "author": "WES Contract SDK Team",
  "license": "Apache-2.0",
  "sdkCompatibility": {
    "go": ">=0.1.0-alpha <0.2.0"
  },
  "sinceSdk": {
    "go": "0.1.0-alpha"
  }
}
